<h1>Coupon Form</h1>
<%= form_tag coupons_path do %>
  <label>Coupon Code:</label>
  <h1><%= text_field_tag :coupon_code %></h1><br>

  <label>Store:</label>
  <h1><%= text_field_tag :store %></h1><br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Submit Coupon" %>
  <%end%>